<Window x:Class="ui_client_example.WriteProgramDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ui_client_example"
        mc:Ignorable="d"
        Title="WriteProgramDialog" Height="350" Width="300">
    <Grid>
        <StackPanel>
            <TextBox Width="300" Height="250" Text="{Binding ProgramText, Mode=TwoWay}" AcceptsReturn="True"/>
            <Button Height="40" Width="100" Content="Start Program" Command="{Binding StartProgramCommand}"/>
        </StackPanel>
    </Grid>
</Window>
